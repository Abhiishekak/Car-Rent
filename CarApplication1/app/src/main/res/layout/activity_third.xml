<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".thirdActivity">


    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="500dp"
        android:text="Book Now!"
        android:textColor="#F44336"
        android:textSize="30sp"
        android:textStyle="bold"
        app:fontFamily="cursive" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="500dp"
        android:background="#F44336"
        android:text="GO"
        android:textColor="@color/colorPrimary1"
        android:textSize="18sp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="164dp"
        android:layout_marginBottom="400dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="600dp"
            android:layout_height="wrap_content"

            android:layout_weight="1"
            app:srcCompat="@drawable/brezza" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="606dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            app:srcCompat="@drawable/tatahexa" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="128dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="262dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"


            android:layout_weight="1"
            app:srcCompat="@drawable/mahindraxuv" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"

            android:layout_weight="1"
            app:srcCompat="@drawable/baleno" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="96dp"
        android:layout_marginTop="400dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="203dp"
            android:layout_height="114dp"


            android:layout_weight="1"
            app:srcCompat="@drawable/bmwvision" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="202dp"
            android:layout_height="114dp"

            android:layout_weight="70"
            app:srcCompat="@drawable/cheveroletcopocamero" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="150dp"
        android:text="Maruthi Suzuki Brezza" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="270dp"
        android:layout_marginTop="150dp"
        android:text="Tata Hexa" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="300dp"
        android:text="Mahindra XUV 500 " />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="240dp"
        android:layout_marginTop="300dp"
        android:text="Maruthi Suzuki Baleno" />


    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="350dp"
        android:text="PREMIUM"
        app:fontFamily="sans-serif-condensed-medium" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="370dp"
        android:text="BMW Vision next 100" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="191dp"
        android:layout_height="32dp"
        android:layout_marginLeft="230dp"
        android:layout_marginTop="370dp"
        android:text="Cheverolet Copo Camero" />

</RelativeLayout>